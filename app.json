{
  "expo": {
    "name": "PowerLists",
    "slug": "PowerLists",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "https",
    "splash": {
      "image": "./assets/splash-light.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.karllasouzza.PowerLists"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "intentFilters": [
          {
            "action": "VIEW",
            "autoVerify": true,
            "data": [
              {
                "scheme": "https",
                "host": "*.powerlists.project"
              }
            ],
            "category": ["BROWSABLE", "DEFAULT"]
          }
        ]
      },
      "package": "com.powerlists.project"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c047d3b0-942b-41f8-b3b5-6f5fe0aec73b"
      }
    }
  }
}
